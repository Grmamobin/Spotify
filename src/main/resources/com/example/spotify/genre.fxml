<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #181818;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.spotify.genre">
   <children>
      <Button fx:id="Indie" layoutX="8.0" layoutY="45.0" mnemonicParsing="false" onAction="#Indie" prefHeight="348.0" prefWidth="192.0" styleClass="btnCliente" stylesheets="@Style.css" text="Indie" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="51.0" />
         </font>
         <effect>
            <Glow />
         </effect></Button>
      <Button fx:id="HipHop" layoutX="212.0" layoutY="261.0" mnemonicParsing="false" onAction="#HipHop" prefHeight="132.0" prefWidth="380.0" styleClass="btnClienteThree" stylesheets="@Style.css" text="Hip-Hop" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="56.0" />
         </font>
         <effect>
            <Glow />
         </effect></Button>
      <Button fx:id="POP" layoutX="212.0" layoutY="45.0" mnemonicParsing="false" onAction="#POP" prefHeight="208.0" prefWidth="380.0" styleClass="btnClienteTwo" stylesheets="@Style.css" text="Pop" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="78.0" />
         </font></Button>
      <Button fx:id="back" layoutX="25.0" layoutY="2.0" mnemonicParsing="false" onAction="#back" prefHeight="0.0" prefWidth="0.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="52.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Pictures/left.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
