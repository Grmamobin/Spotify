<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="150.0" prefWidth="500.0" style="-fx-background-color: #181818;" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.spotify.createPlayList">
   <children>
      <TextField fx:id="Playadd" layoutX="58.0" layoutY="27.0" onAction="#Playadd" prefHeight="17.0" prefWidth="339.0" promptText="Make Your List" styleClass="textField" stylesheets="@Style.css" />
      <Button fx:id="cancel" layoutX="402.0" layoutY="96.0" mnemonicParsing="false" onAction="#cancel" styleClass="upgrade-button" stylesheets="@Style.css" text="cancel" textFill="#4ce1ca" />
      <Button fx:id="add" layoutX="48.0" layoutY="97.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="62.0" styleClass="upgrade-button" stylesheets="@Style.css" text="add" textFill="#4ce1ca" />
      <RadioButton fx:id="Private" layoutX="176.0" layoutY="102.0" mnemonicParsing="false" onAction="#Private" text="Private" textFill="#4ce1ca" />
      <RadioButton fx:id="Public" layoutX="250.0" layoutY="101.0" mnemonicParsing="false" onAction="#Public" text="Public" textFill="#4ce1ca" />
   </children>
</AnchorPane>
